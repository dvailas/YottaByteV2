<h1>User#create</h1>
<p>Find me in app/views/user/create.html.erb</p>

<%= form_with scope: :user, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :email %><br>
    <%= form.text_area :email %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </p>

  <p>
    <%= form.label :Address %><br>
    <%= form.text_field :Address %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>